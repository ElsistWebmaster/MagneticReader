					<sourceCode type="ST">
						<![CDATA[(* ************************************************************************** *)
(* FUNCTION BLOCK "MagneticReader"                                            *)
(* ************************************************************************** *)
(* Questa FB gestisce un semplice protocollo di comunicazione binario.        *)
(*                                                                            *)
(* Il numero di dati da acquisire è:                                          *)
(* Traccia ISO 1, SS:% IATA, massimo 70 caratteri a 7 bit;                    *)
(* Traccia ISO 2, SS:; ABA, massimo 40 caratteri a 5 bit;                     *)
(* Traccia ISO 3, SS:; MINTS massimo 107 caratteri a 5 bit.                   *)
(* Totale 217 bytes di dato                                                   *)
(* Cks 2 bytes per traccia,                                                   *)
(* <CR> 1 byte per traccia                                                    *)
(* <LF> finale                                                                *)
(* -------------------------------------------------------------------------- *)

    (* ---------------------------------------------------------------------- *)
    (* INIZIALIZZAZIONI                                                       *)
    (* ---------------------------------------------------------------------- *)
	(* Eseguo Spy per averlo attivo da Telnet. *)

	IF (SpyOn) THEN IF NOT(SysSpyData(0, 0, 0, 0)) THEN RETURN; END_IF; END_IF;

    (* Eseguo gestione variabili one shot. *)

	IF (Done OR Fault) THEN CaseNr:=0; END_IF; 
	FOR i:=0 TO 2 DO TRcvd[i]:=FALSE; END_FOR;
	Done:=FALSE; (* Execution done *)
	Fault:=FALSE; (* Execution fault*)

	(* ---------------------------------------------------------------------- *)
	(* FB ENABLE                                                              *)
	(* ---------------------------------------------------------------------- *)
	(* Check if the FB is enabled. *)

	IF NOT(Enable) THEN CaseNr:=0; RETURN; END_IF;

    (* ---------------------------------------------------------------------- *)
    (* ESEGUO GESTIONE ERRORE                                                 *)
    (* ---------------------------------------------------------------------- *)
	(* Eseguo gestione report errore. *)

	IF (ErrorNr <> 0) THEN
		IF (SpyOn) THEN
			i:=SysVarsnprintf(ADR(DFrame), SIZEOF(DFrame), 'Error:%08d', UDINT_TYPE, ADR(ErrorNr));
			i:=SysLWVarsnprintf(ADR(DFrame), SIZEOF(DFrame), ', On Case:%d', USINT_TYPE, ADR(CaseNr));
			i:=SysSpyData(SPY_ASCII, 16#40000000, ADR('Er'), ADR(DFrame));
		END_IF;

		(* Impulso di "Fault". *)

		ErrorNr:=0; (* Error number *)
		Fault:=TRUE; (* Execution fault*)
		RETURN;
	END_IF;

    (* ---------------------------------------------------------------------- *)
    (* ESEGUO CONTROLLO SE TIMEOUT GESTIONE                                   *)
    (* ---------------------------------------------------------------------- *)
	(* Non è possibile restare in un case per tempo superiore al timeout. *)

    IF (CaseNr = 0) THEN TimeBf:=SysGetSysTime(TRUE); END_IF;
    IF ((SysGetSysTime(TRUE)-TimeBf) > 1000000) THEN ErrorNr:=100; RETURN; END_IF;

    (* ---------------------------------------------------------------------- *)
    (* CASES GESTIONE                                                         *)
    (* ---------------------------------------------------------------------- *)
	(* Cases gestione programma. *)

	CASE (CaseNr) OF

		(* ------------------------------------------------------------------ *)
		(* GESTIONE RICEZIONE FRAME                                           *)
		(* ------------------------------------------------------------------ *)
		(* Eseguo inizializzazioni. *)

		0:
		IF NOT(TO_BOOL(SysGetIChars(Fp))) THEN RETURN; END_IF;
		i:=Sysmemset(ADR(DFrame), 0, SIZEOF(DFrame));
		i:=Sysmemset(ADR(TLength), 0, SIZEOF(TLength));
		Ptr[0]:=ADR(DFrame); (* Auxiliary pointer *)
		CaseNr:=CaseNr+1; (* Case gestione programma *)

		(* ------------------------------------------------------------------ *)
		(* Eseguo ricezione dati ed attendo <LF>. *)

		1:
		WHILE (TO_BOOL(SysGetIChars(Fp))) DO
			IF (TO_UDINT(Ptr[0]) > TO_UDINT(ADR(DFrame)+SIZEOF(DFrame)-1)) THEN ErrorNr:=110; RETURN; END_IF;
			@Ptr[0]:=TO_BYTE(Sysfgetc(Fp)); (* Rx/Tx character *)
			IF (@Ptr[0] = 16#0A) THEN CaseNr:=CaseNr+1; RETURN; END_IF;
			Ptr[0]:=Ptr[0]+1; (* Auxiliary pointer *)
		END_WHILE;

		(* ------------------------------------------------------------------ *)
		(* Eseguo spionaggio dato ricevuto. *)

		2:
		IF (SpyOn) THEN i:=SysSpyData(SPY_ASCII, 16#00000001, ADR('Rx'), ADR(DFrame)); END_IF;
		Ptr[0]:=ADR(DFrame); (* Auxiliary pointer *)
		CaseNr:=10; (* Case gestione programma *)

		(* ------------------------------------------------------------------ *)
		(* TRACCIA 1                                                          *)
		(* ------------------------------------------------------------------ *)
		(* IATA (International Air Transportation Association), massimo 70    *)
		(* caratteri a 7 bit. Inizia con "%" e termina con "?".               *)
		(* ------------------------------------------------------------------ *)
		(* Ricerco inizio dati traccia (Start sentinel "%"). *)
		(* Ptr[0] puntato per ricerca dati traccia successiva. *)

		10:
		Ptr[1]:=SysStrFind(Ptr[0], ADR('%'), FIND_GET_END);
		IF (Ptr[1] = NULL) THEN CaseNr:=20; RETURN; END_IF; 
		Ptr[0]:=SysStrFind(Ptr[0], ADR('$r'), FIND_GET_END);

		(* Ricerco fine dati traccia (End sentinel "?"). *)

		Ptr[2]:=SysStrFind(Ptr[1], ADR('?'), FIND_DEFAULT);
		IF (Ptr[2] = NULL) THEN ErrorNr:=1000; RETURN; END_IF;
		TLength[0]:=Ptr[2]-Ptr[1]; (* Track length *)

		(* Trasferisco dato traccia in buffer utente. *)

		IF (TBuffer[0] = NULL) THEN ErrorNr:=1010; RETURN; END_IF;
		i:=Sysmemmove(TBuffer[0], Ptr[1], TLength[0]);
		TRcvd[0]:=TRUE; (* Track received *)
		CaseNr:=20; (* Case gestione programma *)

		(* ------------------------------------------------------------------ *)
		(* TRACCIA 2                                                          *)
		(* ------------------------------------------------------------------ *)
		(* ABA (American Bankers Association), massimo 40 caratteri a 5 bit.  *)
		(* Inizia con ";" e termina con "?".                                  *)
		(* ------------------------------------------------------------------ *)
		(* Ricerco inizio dati traccia (Start sentinel ";"). *)
		(* Ptr[0] puntato per ricerca dati traccia successiva. *)

		20:
		Ptr[1]:=SysStrFind(Ptr[0], ADR(';'), FIND_GET_END);
		IF (Ptr[1] = NULL) THEN CaseNr:=30; RETURN; END_IF; 
		Ptr[0]:=SysStrFind(Ptr[0], ADR('$r'), FIND_GET_END);
	
		(* Ricerco fine dati traccia (End sentinel "?"). *)

		Ptr[2]:=SysStrFind(Ptr[1], ADR('?'), FIND_DEFAULT);
		IF (Ptr[2] = NULL) THEN ErrorNr:=1100; RETURN; END_IF;
		TLength[1]:=Ptr[2]-Ptr[1]; (* Track length *)

		(* Trasferisco dato traccia in buffer utente. *)

		IF (TBuffer[1] = NULL) THEN ErrorNr:=1110; RETURN; END_IF;
		i:=Sysmemmove(TBuffer[1], Ptr[1], TLength[1]);
		TRcvd[1]:=TRUE; (* Track received *)
		CaseNr:=30; (* Case gestione programma *)

		(* ------------------------------------------------------------------ *)
		(* TRACCIA 3                                                          *)
		(* ------------------------------------------------------------------ *)
		(* MINTS (Mutual Institution National Transfer System), massimo 107   *)
		(* caratteri a 5 bit. Inizia con ";" e termina con "?".               *)
		(* ------------------------------------------------------------------ *)
		(* Ricerco inizio dati traccia (Start sentinel ";"). *)

		30:
		Ptr[1]:=SysStrFind(Ptr[0], ADR(';'), FIND_GET_END);
		IF (Ptr[1] = NULL) THEN CaseNr:=0; RETURN; END_IF; 

		(* Ricerco fine dati traccia (End sentinel "?"). *)

		Ptr[2]:=SysStrFind(Ptr[1], ADR('?'), FIND_DEFAULT);
		IF (Ptr[2] = NULL) THEN ErrorNr:=1200; RETURN; END_IF;
		TLength[2]:=Ptr[2]-Ptr[1]; (* Track length *)

		(* Trasferisco dato traccia in buffer utente. *)

		IF (TBuffer[2] = NULL) THEN ErrorNr:=1210; RETURN; END_IF;
		i:=Sysmemmove(TBuffer[2], Ptr[1], TLength[2]);
		TRcvd[2]:=TRUE; (* Track received *)
		CaseNr:=0; (* Case gestione programma *)
		ELSE
			ErrorNr:=110; (* Error number *)
	END_CASE;
	
(* [End of file] *)

]]>
